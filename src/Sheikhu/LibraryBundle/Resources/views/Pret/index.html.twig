{% extends '@SheikhuLibrary/layout.html.twig' %}

    {% block content -%}

    {% embed '@SheikhuLibrary/Embed/grid.html.twig' with { size: 4} %}

    {% block title %}
    Pret list
    {% endblock %}

    {% block grid_content %}
    <table class="mws-table">
        <thead>
        <tr>
                <th>Id</th>
                <th>Datepret</th>
                <th>Dateretourprevue</th>
                <th>Dateretour</th>
                <th>Etat</th>
            <th>Actions</th>
        </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
        <tr>
                    <td><a href="{{ path('prets_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                    <td>{% if entity.datePret %}{{ entity.datePret|date('Y-m-d H:i:s') }}{% endif %}</td>
                    <td>{% if entity.dateRetourPrevue %}{{ entity.dateRetourPrevue|date('Y-m-d H:i:s') }}{% endif %}</td>
                    <td>{% if entity.dateRetour %}{{ entity.dateRetour|date('Y-m-d H:i:s') }}{% endif %}</td>
                    <td>{{ entity.etat }}</td>
                    <td>
                <ul>
                    <li>
                        <a href="{{ path('prets_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('prets_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                    </td>
        </tr>
        {% endfor %}
        </tbody>
    </table>
    {% endblock %}

    {% endembed %}

    {% endblock %}
